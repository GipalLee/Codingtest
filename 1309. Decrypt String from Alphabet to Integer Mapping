class Solution:
    def freqAlphabets(self, s: str) -> str: 
        dic = {'1':'a','2':'b','3':'c','4':'d','5':'e','6':'f','7':'g','8':'h','9':'i','10':'j','11':'k','12':'l','13':'m','14':'n','15':'o','16':'p','17':'q','18':'r','19':'s','20':'t','21':'u','22':'v','23':'w','24':'x','25':'y','26':'z'}
        len_s, i, ans = len(s), 0, ''
        while i < len_s:                                    # 인덱스가 총 길이보다 전까지 반복
            if i+2 < len_s and s[i+2] == '#':               # 만약 해당 인덱스 + 2글자에 #이 있다면
                ans += dic[s[i:i+2]]                        # 정답에 #앞 2글자를 사전에서 찾아서 더함
                i += 3                                      # 인덱스는 #과 숫자 를 합쳐 총 3인덱스 이동
            else: 
                ans += dic[s[i]]                            # 해당하는 값이 단일 인덱스라면 사전에서 찾아 값을 더한 후
                i += 1                                      # 1칸 이동
        return ans
