class Solution:
    def nextGreatestLetter(self, letters: List[str], target: str) -> str:
        alph = ["a","b","c","d","e","f","g","h","i","j","k","l","m","n","o","p","q","r","s","t","u","v","w","x","y","z"]
        
        for i in letters :
            if alph.index(i) > alph.index(target) :
                return i
          
        return letters[0]
