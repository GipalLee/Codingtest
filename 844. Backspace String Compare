class Solution:
    def backspaceCompare(self, S: str, T: str) -> bool:
        if S is None:
            return False
        if T is None:
            return False
            
        res = []
        res1 = []
        
        for i in S :
            if i == # :
                res.pop()
                
            else :
                res.append(i)
                
         for i in T :
            if i == # :
                res1.pop()
                
            else :
                res1.append(i)
                
         if res == res1 :
            return True
         else :
            return False
                
         
