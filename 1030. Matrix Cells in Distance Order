class Solution(object):
    def allCellsDistOrder(self, R, C, r0, c0):
        ans, dist = [], []
        
        for i in range(R) :
          for j in range(C) :
            dist += [abs(r-r0) + abs(c-c0)
            ans += [[r,c]]
            
            dist, ans = sorted(zip(dist,ans))
            return ans
