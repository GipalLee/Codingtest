class Solution:
    def generateTheString(self, n: int) -> str:
        ans = ''
        
        if n % 2 == 0 :                    # 짝수 일 경우
            for _ in range(n-1) :          # 1 뺀 만큼 a 를 입력
                ans += 'a'
            ans += 'b'                     # 마지막에 b 한 개 입력
            return ans
        
        else :
            for _ in range(n) :           # 홀수 일 경우
                ans += 'a'                # a로 다채움
            return ans    
            
