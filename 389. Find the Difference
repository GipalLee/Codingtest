class Solution:
    def findTheDifference(self, s: str, t: str) -> str:
        a = Counter(s)
        b = Counter(t)
       
        
        for i in a :
            
            if a[i] != b[i] :
                return i
            
        for i in b :
            
            if a[i] != b[i] :
                return i
