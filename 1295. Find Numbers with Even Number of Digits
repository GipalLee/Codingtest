class Solution:
    def findNumbers(self, nums: List[int]) -> int:
        count = 0
        
        for i in nums :               # 숫자를 한개씩 뽑음
            i = len(str(i))           # 정수형을 문자형으로 바꾼 뒤 총길이
            if i % 2 == 0 :           # 짝수인지 확인
                count += 1 
                
        return count
