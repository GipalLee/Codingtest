class Solution:
    def shuffle(self, nums: List[int], n: int) -> List[int]:
        a = nums[:len(nums)//2]
        b = nums[len(nums)//2:]
        answer = []
        for i in range(len(nums)//2) :
            answer.append(a[i])
            answer.append(b[i])
            
        return answer
    
